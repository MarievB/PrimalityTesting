<Group hide-input="false" labelreference="L80" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">trialDivision := proc(n)
  local i;
  description &quot;Return if n is prime or not using trial division.&quot;;
  if n = 0 or n = 1 then 
    return 0;
  end if;
  for i from 2 to trunc(sqrt(n)) do
    if n mod i = 0 then
      return 0;
    end if;
  end do;
  return 1;
end:
</Text-field>
</Input>
