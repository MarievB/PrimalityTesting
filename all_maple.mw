<Group labelreference="L189" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">findR := proc(n)
  local q, flag, n_q, j, fllgn2, r;
  description &quot;Return r matching input n w.r.t. AKS.&quot;;
  r:=0;
  fllgn2 := trunc((log[2](n))^2);

  for q from fllgn2 to n-1 do 
    flag := true;
    n_q := n mod q; 

    for j from 1 to fllgn2 do     
      if Power(n_q, j) mod q = 1 then
        flag := false; 
        break;
      end if;
    end do; 
    if flag then 
      r:= q; 
      break; 
    end if; 
  end do; 
  return r;
end:</Text-field>
</Input>
</Group>
