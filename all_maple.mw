<Group hide-input="false" labelreference="L82" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">MillerRabin := proc(n)
  local t, s, b, y, i, random_int; 
  description &quot;Return if n is prime or not using the Miller-Rabin algorithm&quot;;
  
  t := n-1;
  s := 0;

  do 
    t := t / 2; 
    s := s + 1;
  until t mod 2 &lt;&gt; 0;

  random_int := rand(1..n-1);
  b := random_int();
  y := Power(b, t) mod n;

  if (y-1) mod n = 0 then 
    return true; 
  end if;

  for i from 1 to s do 
    if (y+1) mod n = 0 then 
      return true; 
    end if; 
    y := Power(y,2) mod n;
  end do;

  return false;

end:</Text-field>
</Input>
</Group>
