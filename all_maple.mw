<Group labelreference="L98" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">isPerfectPower := proc(n)
  local A, b,i;
  description &quot;Return if n is a perfect power.&quot;;
  A := [seq(i, i=2..trunc(sqrt(n)))];
  for b from 2 to trunc(log[2](n)) do;
    if isPowerOf(n, b, A, 0, numelems(A)) then;
      return true;
    end if;
  end do;
  return false;
end:</Text-field>
</Input>
</Group>
