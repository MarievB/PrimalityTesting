<Group hide-input="false" labelreference="L81" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">main := proc()
  local a, b, i, j, n, main_tic, main_toc, err_aks, err_mr, repetitions, steps, stepsize, t, T, p_mr, p_aks, p_td; 
  
  main_tic := time();
  err_aks := 0;
  err_mr := 0; 

  repetitions := 5;
  steps := 100;
  stepsize := 100; # even number no less than 22
  
  T := Matrix(1..steps+1,3);
  T[1,1] := &quot;mr&quot;;
  T[1,2] := &quot;aks&quot;;
  T[1,3] := &quot;td&quot;;

  for i from 1 to steps do 
    a := i*stepsize + 1; 
    b := (i+1)*stepsize;
    
    t, p_mr  := run(itMillerRabin, repetitions, a, b);
    T[i+1,1]  := t;
    t, p_aks := run(AKS, repetitions, a, b);
    T[i+1,2] := t;
    t, p_td  := run(trialDivision, repetitions, a, b);
    T[i+1,3] := t;
    err_mr  := err_mr  + abs(p_mr  - p_td);
    err_aks := err_aks + abs(p_aks - p_td);
  end do; 

  main_toc := time();
  print(&quot;time elapsed: &quot;, main_toc-main_tic);
  print(&quot;error, MR: &quot;, err_mr);
  print(&quot;error, AKS: &quot;, err_aks);
  Export(&quot;<Font encoding="UTF-8">data\134\134all_maple.csv&quot;, T, base = currentdir);
</Font>  
end:</Text-field>
</Input>
</Group>
