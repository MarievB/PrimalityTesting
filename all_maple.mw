<Group hide-input="false" labelreference="L112" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">itMillerRabin := proc(n) 
  local i, m;
  description &quot;Return if n is prime or not using m iterations of the Miller-Rabin algorithm&quot;;
  m := 4;   # in order for the generic run() function to work, m is not taken as input.

  for i from 1 to m do 
    if not MillerRabin(n) then 
      return 0;
    end if;
  end do;
  return 1;
end:</Text-field>
</Input>
