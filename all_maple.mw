<Group hide-input="false" labelreference="L116" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">run := proc(algorithm, repetitions, a, b)
  local m, i, j, tic, toc, primes;

  tic := time();
  primes := 0;
  for i from a by 2 to b do
    for j from 1 to repetitions do 
      primes := primes + algorithm(i); 
    end do;
  end do ;
  toc := time();

  return (toc-tic)/repetitions, primes;
end:</Text-field>
</Input>
</Group>
