<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">binomialCheck := proc(n, r, a_max)
  local a, rhs, lhs; 
  description &quot;Return if (x+a)^n = x^n + a mod (n, x^r-1).&quot;;
  rhs := Powmod(x, n, x^r-1, x)+1 mod n;
  for a from 1 to a_max do
    lhs  := Powmod(x+a, n, x^r-1 ,x) mod n;
    if(lhs &lt;&gt; rhs) then 
      return false;
    fi;
    rhs := rhs + 1;
  od;
  return true; 
end:</Text-field>
</Input>
