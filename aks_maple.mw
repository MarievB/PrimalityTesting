<Group hide-input="false" labelreference="L220" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">AKS := proc(n)
  local a, j, r, q, n_q, flag, fllgn2, a_max;
  description &quot;Return if n is prime or not using the AKS algorithm.&quot;;

  if isPerfectPower(n) then 
    return 0; 
  end if; 

  r := findR(n);
  if r = 0 then 
    return -1;
  fi;

  for a from 2 to r do 
    if GCD(a,n) &gt; 1 then
      return 0;
    end if; 
  end do; 

  a_max := trunc(sqrt(r)*log[2](n));
  if not binomialCheck(n, r, a_max)  then 
    return 0; 
  fi;

  return 1;

end proc:</Text-field>
</Input>
</Group>
