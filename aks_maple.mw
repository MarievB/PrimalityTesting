<Group labelreference="L214" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">isPowerOf := proc(n, b, A, i, j) 
  local m, x; 
  description &quot;Return if A[i..j] contains a base a such that a^b = n.&quot;;
  m := trunc((j-i)/2);
  x := A[i+m+1]^b; 
  if x = n then 
    return true; 
  elif j - i &lt;= 1 then 
    return false; 
  elif x &lt; n then 
    return isPowerOf(n, b, A, i+m, j); 
  else 
    return isPowerOf(n, b, A, i, i+m); 
  end if; 
end proc:</Text-field>
</Input>
</Group>
